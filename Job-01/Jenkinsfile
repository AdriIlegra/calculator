pipeline {
    agent any
    stages {
        stage('Upload to Artifactory') {
            steps {
                script {
                    def server = Artifactory.server 'calculadora'
                    def uploadSpec = """{
                        "files": [

                            {
                                "pattern": "build/libs/tema-04.0.0.1-SNAPSHOT.jar",
                                "target": "calculadora/"
                            }
                        ]
                    }"""
                    server.upload(uploadSpec, buildInfo)
                }
            }
        }
    }
}
