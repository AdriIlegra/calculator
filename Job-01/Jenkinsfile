pipeline {
    agent any

    stages {

        stage('Build') {
            steps {
                sh './gradlew clean build'
            }
        }

        stage('Artifactory') {
            steps {
                rtUpload(
                    serverId: "tema-final-01",
                    spec: """{
                        "files": [
                        {
                            "pattern": "build/libs/*.jar",
                            "target": "tema-final-01/"
                        }
                        ]
                    } """


                    )
            }
        }

    }
}