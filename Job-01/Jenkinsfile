stage('Artifactory') {
    steps {
        script {
            def server = Artifactory.server 'artifactory'
            def uploadSpec = """{
                "files": [
                    {
                        "pattern": "target/*.jar",
                        "target": "libs-release-local"
                    }
                ]
            }"""
            server.upload(uploadSpec)
        }
    }
}
