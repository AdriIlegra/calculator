pipeline {
    agent any
    tools {
        jfrog 'Jfrog-cli'
    }
environment {
    JFROG_CLI_HOME = "${env.WORKSPACE}/.jfrog"
}

stages {
    stage('Build') {
        steps {
            sh 'echo "Building.."'
        }
    }
    stage('Test') {
        steps {
            sh 'echo "Testing.."'
        }
    }
    stage('Deploy') {
        steps {
            sh 'echo "Deploying.."'
        }
    }
}